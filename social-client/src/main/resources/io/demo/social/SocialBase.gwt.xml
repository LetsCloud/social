<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="social">
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Other module inherits -->
	<inherits name='com.github.nalukit.nalu.Nalu' />
	<inherits
		name='com.github.nalukit.nalu.plugin.elemental2.NaluPluginElemental2' />

	<inherits name='org.dominokit.domino.ui.DominoUI' />

	<!-- Specify the app entry point class. -->
	<entry-point class='io.demo.social.client.Social' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

	<add-linker name="xsiframe" />
</module>